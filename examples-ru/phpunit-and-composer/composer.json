{
    "name": "draculaid/phpmocker-examples",
    "description": "Example for PhpMocker: PhpUnit and Composer class autoloader",
    "type": "library",
    "authors": [
        {
            "name": "Konstantin Marataev",
            "email": "dracul.aid@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DraculAid\\PhpMocker\\": "../../src/",
            "DraculAid\\PhpMockerExamples\\Classes\\": "classes/",
            "DraculAid\\PhpMockerExamples\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
